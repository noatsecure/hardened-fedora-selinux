module hardhat-selinux 1.0.0;
########################################
require {
    class file { execute map };
    type hardened-malloc_t;
    type load_policy_t;
    type setfiles_t;
}
########################################
#============= load_policy_t ==============
allow load_policy_t hardened-malloc_t:file map;
#============= setfiles_t ==============
allow setfiles_t hardened-malloc_t:file { execute map };
