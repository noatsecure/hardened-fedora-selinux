    type gpg_secret_t;
    type home_bin_t;
    type home_cert_t;
    type home_root_t;
    type http_port_t;
    type iceauth_exec_t;
    type iceauth_home_t;
    type init_t;
    type init_var_run_t;
    type ld_so_cache_t;
