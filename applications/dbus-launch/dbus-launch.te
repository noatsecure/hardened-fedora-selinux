########################################
type dbus-launch_t, application_domain_type;
type dbus-launch_exec_t, application_exec_type, entry_type, exec_type, file_type, non_auth_file_type, non_security_file_type;
role user_r types dbus-launch_t;
type_transition user_t dbus-launch_exec_t: process dbus-launch_t;
